meMA.Kontakion1.incipit = "…"
meMA.Kontakion1.text = ""

meMA.Ode7C1H.ode = miscellanea_en_US_acook.misc.Ode7
meMA.Ode7C1H.mode = miscellanea_en_US_acook.misc.Mode6
meMA.Ode7C1H.greekmelody = he.h.m6_en_US_acook.heHE.DrosovolonMenTinKaminon.greekname
meMA.Ode7C1H.melody = he.h.m6_en_US_acook.heHE.DrosovolonMenTinKaminon.melody
meMA.Ode7C1H.incipit = he.h.m6_en_US_acook.heHE.DrosovolonMenTinKaminon.incipit
meMA.Ode7C1H.text = he.h.m6_en_US_acook.heHE.DrosovolonMenTinKaminon.text

meMA.Ode7C11.ode = miscellanea_en_US_acook.misc.Ode7
meMA.Ode7C11.mode = miscellanea_en_US_acook.misc.Mode6
meMA.Ode7C11.greekmelody = he.h.m6_en_US_acook.heHE.DrosovolonMenTinKaminon.greekname
meMA.Ode7C11.melody = he.h.m6_en_US_acook.heHE.DrosovolonMenTinKaminon.name
meMA.Ode7C11.incipit = "Brilliant, blazing bright…"
meMA.Ode7C11.text = "Brilliant, blazing bright your life became, divinely-wise * through your ascetic prayer and work * so your martyrdom * being bitter was announced abroad * to Christ you cried out and glorified: ‘of all our fathers you are God; blessed are you, O God.’"

meMA.Ode7C12.ode = miscellanea_en_US_acook.misc.Ode7
meMA.Ode7C12.mode = miscellanea_en_US_acook.misc.Mode6
meMA.Ode7C12.melody = he.h.m6_en_US_acook.heHE.DrosovolonMenTinKaminon.name
meMA.Ode7C12.incipit = "…"
meMA.Ode7C12.text = "You have found a belt to gird yourself with purity; * prudence and manly virtue are yours. * And you gained the crown * very bright of martyrdom as well * by grace, you cried out with godly words: \"Of all our fathers you are God; blessed are you, O God.\""

meMA.Ode7C13.ode = miscellanea_en_US_acook.misc.Ode7
meMA.Ode7C13.mode = miscellanea_en_US_acook.misc.Mode6
meMA.Ode7C13.melody = he.h.m6_en_US_acook.heHE.DrosovolonMenTinKaminon.name
meMA.Ode7C13.incipit = "…"
meMA.Ode7C13.text = "The irreverent refused to offer honor to * you, Savior, in your icon most pure * and they terribly * whipped and flayed your servant so devout * as he sang out hymns to you and cried: \"Of all our fathers you are God; blessed are you, O God.\""

meMA.Ode7C1T.ode = miscellanea_en_US_acook.misc.Ode7
meMA.Ode7C1T.mode = miscellanea_en_US_acook.misc.Mode6
meMA.Ode7C1T.melody = he.h.m6_en_US_acook.heHE.DrosovolonMenTinKaminon.name
meMA.Ode7C1T.incipit = "…"
meMA.Ode7C1T.text = "We who celebrate your giving birth, O lady pure * all magnify you, calling you blest * for the Son you bore * has delivered all those born of flesh * protecting us so we cry to him: \"Of all our fathers you are God; blessed are you, O God.\""

meMA.Ode8C1H.ode = miscellanea_en_US_acook.misc.Ode8
meMA.Ode8C1H.mode = miscellanea_en_US_acook.misc.Mode6
meMA.Ode8C1H.greekmelody = he.h.m6_en_US_acook.heHE.EkFlogosToisOsiois.greekname
meMA.Ode8C1H.melody = he.h.m6_en_US_acook.heHE.EkFlogosToisOsiois.melody
